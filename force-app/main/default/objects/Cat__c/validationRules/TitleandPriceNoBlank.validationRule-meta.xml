<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TitleandPriceNoBlank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Isblank( Title__c ),Isblank( Noofcopy__c ))</errorConditionFormula>
    <errorDisplayField>Title__c</errorDisplayField>
    <errorMessage>Title can not be blank and Price Cannot be Blank</errorMessage>
</ValidationRule>
